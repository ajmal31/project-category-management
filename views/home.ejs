    <div class="container">
        <div class="tab-content wow fadeIn animated" id="myTabContent">
            <div class="tab-pane fade show active" id="tab-one" role="tabpanel" aria-labelledby="tab-one">

                <div class="row product-grid-4">

                    <% data.forEach(element=> { %>
                        <div class="col-lg-3 col-md-3 col-12 col-sm-6 mb-3">

                            <div class="product-cart-wrap">

                                <div class="product-img-action-wrap">
                                    <div class="product-img product-img-zoom" style="height: 5cm;">
                                        <a href="/productDetails/<%=element._id%> ">
                                            <img class="default-img" src="/images/<%=element.filename[0]%>" alt="">
                                            <img class="hover-img" src="/images/<%=element.filename[0]%>" alt="">
                                        </a>
                                    </div>

                                </div>
                                <div class="product-content-wrap" style="height: 5cm;">
                                    <div class="product-category">
                                        <a href="">
                                            <%=element.title%>
                                        </a>
                                        <h2><a href="#">
                                                <%=element.category %>
                                            </a></h2>
                                    </div>
                                    <h4><a href="#">
                                            <%=element.description %>
                                        </a></h4>

                                    <div class="rating-result" title="90%">
                                        <span>
                                            <span>86%</span>
                                        </span>
                                    </div>

                                    <div class="product-price">
                                        <span style="color: green;">
                                            <%=element.price%>
                                        </span>
                                        <span class="old-price">$245.8</span>
                                        <!-- <span style="color: green;">$2465</span> -->
                                    </div>
                                    <div class="product-action-1 show">
                                        <a aria-label="Add To Cart" class="action-btn hover-up" href="#"><i
                                                class="fi-rs-shopping-bag-add"></i></a>
                                    </div>
                                </div>

                            </div>

                        </div>
                        <% }) %>





                </div>
            </div>
        </div>
    </div>
    <%- include('./partials/user-footer') %>